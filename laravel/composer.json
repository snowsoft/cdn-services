{
    "name": "cdn-services/laravel-adapter",
    "description": "Laravel Storage adapter for CDN Services Node.js backend",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "CDN Services",
            "email": "info@cdn-services.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/filesystem": "^9.0|^10.0|^11.0",
        "illuminate/http": "^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "CdnServices\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CdnServices\\CdnServicesServiceProvider"
            ],
            "aliases": {
                "CdnServices": "CdnServices\\Facades\\CdnServices"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

